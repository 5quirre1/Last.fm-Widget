{
    "version": 2,
    "builds": [
      {
        "src": "api/**",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/playing",
        "dest": "/api/playing.js"
      },
      {
        "src": "/api/playingFRAME",
        "dest": "/api/serveHTML.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  
